- alias: 'Powerfail'
  trigger:
    platform: state
    entity_id: device_tracker.powerfail
    to: 'not_home'
  action:
    service: notify.simplepush
    data:
      message: "Stromausfall im Stall!"

- alias: 'Power back'
  trigger:
    platform: state
    entity_id: device_tracker.powerfail
    to: 'home'
  action:
    service: notify.simplepush
    data:
      message: "Stromausfall vorbei"